{
  "name": "changeling",
  "version": "1.1.0",
  "description": "Generates changelogs between commit hashes and creates a clean markdown file sections from commit message prefixes",
  "main": "cli.js",
  "scripts": {
    "app": "./cli.js",
    "test": "xo"
  },
  "bin": {
    "clog": "./cli.js",
    "changelog": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/excalith/changeling"
  },
  "author": "Can Cellek",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "fs": "0.0.1-security",
    "inquirer": "^6.2.0",
    "node-cmd": "^3.0.0"
  },
  "devDependencies": {
    "configstore": "^4.0.0",
    "husky": "^1.1.1",
    "opn": "^5.4.0",
    "xo": "^0.23.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "xo": {
    "prettier": true,
    "space": false,
    "semicolon": false,
    "singleQuote": true,
    "trailingComma": false
  }
}
